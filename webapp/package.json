{
  "name": "fhir-demo",
  "version": "1.0.0",
  "repository": "http://github.com/awatson1978/fire-demo",
  "description": "Application description.",
  "scripts": {
    "test": "meteor test --driver-package practicalmeteor:mocha --port 3030",
    "test-app": "meteor test --full-app --driver-package practicalmeteor:mocha --port 3030 --once",
    "test-packages": "meteor test-packages",
    "circle": "NODE_ENV=circle meteor --settings settings.cirlce.json",
    "package-verification": "starrynight run-tests --type package-verification",
    "verification": "starrynight run-tests --type verification",
    "validation": "starrynight run-tests --type validation",
    "spacejam": "starrynight run-tests --framework spacejam",
    "deploy-galaxy": "DEPLOY_HOSTNAME=galaxy.meteor.com meteor deploy meteor-on-fhir.meteorapp.com --settings settings.galaxy.json",
    "coverage": "COVERAGE=1 meteor",
    "monitor": "COVERAGE=1 meteor test --driver-package dispatch:mocha --port 5000",
    "nightwatch": "nightwatch -c .meteor/nightwatch.json",
    "gagarin": "starrynight run-tests --type verification",
    "phantom-nightwatch": "nightwatch -c .meteor/nightwatch.json --env phantom",
    "phantom-mocha": "meteor test --once --driver-package dispatch:mocha-phantomjs --port 3030",
    "phantom-mocha-app": "meteor test --full-app --driver-package dispatch:mocha-phantomjs --port 3030 --once"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-meteor": "^6.12.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "chromedriver": "2.19.0",
    "eslint": "^2.13.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-meteor": "^3.5.2",
    "eslint-plugin-react": "^4.3.0",
    "faker": "^3.1.0",
    "nightwatch": "0.9.8",
    "phantomjs-prebuilt": "2.1.12",
    "selenium-server-standalone-jar": "2.45.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "plugins": [
      "react",
      "es2016",
      "meteor"
    ],
    "extends": [],
    "globals": {
      "server": false,
      "browser": false,
      "expect": false
    },
    "rules": {
      "import/no-unresolved": 0,
      "react/jsx-uses-vars": 1
    }
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "eslint": "^2.13.1",
    "eslint-config-eslint": "^3.0.0",
    "eslint-plugin-meteor": "^3.6.0",
    "eslint-plugin-react": "^4.3.0",
    "faker": "^3.1.0",
    "jquery": "^2.2.4",
    "jquery-validation": "^1.15.1",
    "material-ui": "^0.15.4",
    "moment": "github:moment/moment",
    "normalize.css": "^4.2.0",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-pure-render-mixin": "^15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.1",
    "react-highcharts": "^8.4.2",
    "react-komposer": "^1.13.1",
    "react-mixin": "^3.0.5",
    "react-router": "^2.7.0",
    "react-router-bootstrap": "^0.20.1",
    "react-scroll-box": "^0.3.3-beta",
    "react-tap-event-plugin": "^1.0.0",
    "react-tappable": "^0.8.1",
    "react-toolbox": "^1.1.2"
  },
  "cssModules": {
    "extensions": [
      "scss"
    ],
    "globalVariables": [
      "node_modules/react-toolbox/lib/_colors.scss",
      {
        "theme-building": true
      },
      "client/main.scss"
    ],
    "outputJsFilePath": {
      ".*node_modules/.*": "{dirname}/{basename}"
    }
  }
}
